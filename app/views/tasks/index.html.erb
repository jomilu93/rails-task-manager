<!DOCTYPE html>
<html>
<head>
  <title>Tasks Index</title>
</head>
<body>

  <ul>
  <% @tasks.each do |task| %>
    <li><%= link_to task.title, task_path(task)%> </br> </br> <%= link_to "Delete Task",
            task_path(task),
            method: :delete,
            data: { confirm: "Are you sure?" } %></br></li>
  <% end %>
  </ul>

  <p><%= link_to "Create New Task", tasks_new_path%></p>

</body>
</html>
